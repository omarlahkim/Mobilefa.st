{
  "name": "react-native-mmkv",
  "version": "2.12.2",
  "summary": "The fastest key/value storage for React Native. ~30x faster than AsyncStorage! Works on Android, iOS and Web.",
  "homepage": "https://github.com/mrousavy/react-native-mmkv#readme",
  "license": "(MIT AND BSD-3-Clause)",
  "authors": "Marc Rousavy <marcrousavy@hotmail.com> (https://github.com/mrousavy)",
  "platforms": {
    "ios": "11.0",
    "tvos": "12.0",
    "osx": "10.14"
  },
  "source": {
    "git": "https://github.com/mrousavy/react-native-mmkv.git",
    "tag": "2.12.2"
  },
  "source_files": [
    "ios/**/*.{m,mm}",
    "ios/MmkvModule.h",
    "cpp/**/*.{h,cpp}"
  ],
  "preserve_paths": [
    "ios/**/*.h"
  ],
  "dependencies": {
    "MMKV": [
      ">= 1.3.3"
    ],
    "React-Core": [

    ],
    "RCT-Folly": [
      "2022.05.16.00"
    ],
    "glog": [

    ]
  },
  "compiler_flags": "$(inherited) -DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -DFOLLY_CFG_NO_COROUTINES=1 -Wno-comma -Wno-shorten-64-to-32",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/boost\" \"$(PODS_ROOT)/Headers/Private/Yoga\"",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20"
  }
}
